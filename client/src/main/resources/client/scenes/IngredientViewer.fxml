<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.BorderPane?>

<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>
<BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.scenes.IngredientViewerCtrl">
    <top>
        <HBox alignment="CENTER" spacing="10.0">
            <Label fx:id="titleLabel" alignment="CENTER" contentDisplay="CENTER" style="-fx-wrap-text: true;">
                <font>
                    <Font size="15.0" />
                </font>
            </Label>
        </HBox>
    </top>
    <center>
        <VBox alignment="CENTER_LEFT">
            <Label fx:id="nutritionalValueLabel">
                <font>
                    <Font size="14.0" />
                </font>
            </Label>
            <HBox>
                <Label fx:id="proteinLabel" />
                <Text fx:id="proteinValue" />
            </HBox>
            <HBox>
                <Label fx:id="fatLabel" />
                <Text fx:id="fatValue" />
            </HBox>
            <HBox>
                <Label fx:id="carbsLabel" />
                <Text fx:id="carbsValue" />
            </HBox>
        </VBox>
    </center>
</BorderPane>